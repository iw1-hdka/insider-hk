{% extends 'base.html.twig' %}

{% block title %}FaqCategory index{% endblock %}

{% block body %}
    <h1>FaqCategory index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
            </tr>
        </thead>
        <tbody>
        {% for faq_category in faq_categories %}
            <tr>
                <td>{{ faq_category.id }}</td>
                <td>{{ faq_category.label }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
